{
  "name": "web-capstone",
  "version": "1.0.0",
  "description": "New",
  "main": "server.ts",
  "scripts": {
    "build": "tsc --esModuleInterop src/server.ts --outDir ./dist",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/src/server.js'",
    "start": "npm run build && cross-env NODE_ENV=dev DEBUG=app:* node ./dist/src/server.js",
    "start:prod": "npm run build && npm run minify && cross-env NODE_ENV=dev DEBUG=app:main,app:endpoint node ./dist/src/server.js",
    "db:create": "cross-env NODE_ENV=dev db-migrate up",
    "db:destroy": "cross-env NODE_ENV=dev db-migrate reset",
    "test": "cross-env NODE_ENV=test jasmine"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.9",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/pg": "^7.14.7",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.3",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "request": "^2.88.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-shubham16598:shubham16598/web-capstone.git"
  },
  "author": "",
  "license": "ISC"
}
